{
    "name": "vyui/vyui",
    "type": "project",
    "description": "The Vyui Fullstack Framework. Pronounced view-e. (Experimental and Educational).",
    "license": "MIT",
    "keywords": ["framework", "vyui"],
    "require": {
        "php": "^8.0",
        "vyui/framework": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}